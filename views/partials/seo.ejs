<!-- Dynamic SEO Head Tags -->
<title><%= title %> | YR Publication</title>
<meta name="description" content="<%= description %>">
<meta name="keywords" content="<%= keywords %>">
<meta name="author" content="YR Publication">
<meta name="robots" content="index, follow">

<!-- Canonical URL for SEO -->
<link rel="canonical" href="https://buyhatke-candles.onrender.com<%= path %>" />

<!-- Open Graph for social sharing -->
<meta property="og:title" content="<%= title %> | YR Publication" />
<meta property="og:description" content="<%= description %>" />
<meta property="og:url" content="https://buyhatke-candles.onrender.com<%= path %>" />
<meta property="og:image" content="/images/logo.png" />
<meta name="twitter:card" content="summary_large_image" />
